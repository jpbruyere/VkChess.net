	<Template>
		<HorizontalStack Name="ItemsContainer" Spacing="10" Margin="10"/>
	</Template>
	<ItemTemplate DataType="Crow.ToggleCommand">
		<Button Style="icoToggle" Command="{}" />
	</ItemTemplate>
	<ItemTemplate DataType="Crow.Command">
		<Button Style="icoButton" Command="{}" />
	</ItemTemplate>
</Menu>